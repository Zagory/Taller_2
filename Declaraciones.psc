Algoritmo Declaraciones
    Definir a Como Entero;
    Definir d Como Real;
    a <- 1;
    d <- 1.0;
    a <- 46 % 9 + 4 * 4 - 2;
    Escribir(a);
    a <- 45 + 43 % 5 * (23 * 3 % 2);
    Escribir(a);
    a <- (45 + 45 * 50) % (a-1);
    Escribir(a);
    d <- d + 1;
    d <- 1.5 * 3 + d;
    Escribir(d);
    d <- 1.5 * 3 + d;
    d <- d + 1;
    Escribir(d);
    a <- a % trunc(3 / a + 3);
    Escribir(a);
FinAlgoritmo
