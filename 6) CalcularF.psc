Algoritmo CalcularF
	Definir  x como entero; 
	Definir sigma Como Real; 
	Definir i Como Entero; 
	Definir lamdba Como Real; 
	Definir alfa Como Real; 
	Definir f como real; 
	x <- 12; 
	sigma <- 2.1836;
	i <- 3; 
	lamdba <- 1.11695; 
	alfa <- 328.67; 
	f <- 3* (x+sigma*i)/(x^2-i^2)-lamdba*(alfa-13.7); 
	Escribir "El valor de f es: ",f; 
	
FinAlgoritmo

	
